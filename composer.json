{
    "name" : "seffeng/laravel-signature",
    "description" : "PHP laravel extension signature",
    "keywords" : [
        "PHP",
        "laravel",
        "lumen",
        "seffeng",
        "signature"
    ],
    "license" : "MIT",
    "homepage" : "https://github.com/seffeng/laravel-signature",
    "authors" : [{
            "name" : "seffeng",
            "email" : "save.zxf@gmail.com"
        }
    ],
    "require" : {
        "php" : ">=7.1.3",
        "seffeng/signature" : ">=0.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0"
    },
    "autoload" : {
        "psr-4" : {
            "Seffeng\\LaravelSignature\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seffeng\\LaravelSignature\\Tests\\": "./tests"
        }
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Seffeng\\LaravelSignature\\SignatureServiceProvider"
            ]
        }
    },
    "minimum-stability" : "stable"
}